algoritmo "Aula 12 - Soma Parametros"


var
 
  a, b: Inteiro

Procedimento Soma ()

inicio
   
   a <- a + 1
   b <- b + 2
   escreval("Valor de A = ", a)
   escreval("Valor de B = ",b)
   escreval("Soma A + B = ", a+b)
FimProcedimento

inicio

   a<- 4
   b<- 8
   Soma ()
   escreval("Valor de a = ", a)
   escreval("Valor de b = ", b)
fimalgoritmo