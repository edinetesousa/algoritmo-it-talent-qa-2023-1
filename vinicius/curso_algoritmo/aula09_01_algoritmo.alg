algoritmo "Aula 09 do curso guanabara"

var

    valor, contador:inteiro

inicio

    contador <- 0
    escreval("Quer contar ate quanto?")
    leia(valor)
    enquanto (contador<=valor) faca
        escreval (contador)
        contador<-contador+1
    fimenquanto
    escreval("Terminei de contar")

fimalgoritmo