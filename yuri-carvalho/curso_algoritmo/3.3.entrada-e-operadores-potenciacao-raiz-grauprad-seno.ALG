Algoritmo "3.3.entrada-e-operadores"
// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 25/05/2023
Var
   a1, a2, a3, a4, a5 :inteiro

Inicio
   a1<- abs(-50)
   a2<- int(exp(9,5))
   a3<- int(raizQ(81))
   a4<- int(raizQ(90))
   a5<- int(sen(GraupRad(90)))
   escreva(a1, " , ", a2, " , ", a3, " , ", a4, " , ", a5)
Fimalgoritmo