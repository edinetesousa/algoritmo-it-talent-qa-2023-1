Algoritmo "9.5.ESTRUTURAS.DE.REPETIÇÃO.CONTE.ATÉ.ONDE.O.USER.QUISER"

// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 29/05/2023
Var
   i, valor: inteiro


Inicio
   i<- 0
   escreval("Quer contar ate quanto?")
   leia(valor)
   enquanto (i <= valor) faca
      i<- i+1
      escreval(i)
   fimenquanto
   escreval("Terminei de contar")

Fimalgoritmo