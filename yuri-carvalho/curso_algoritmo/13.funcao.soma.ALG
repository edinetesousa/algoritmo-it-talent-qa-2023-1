Algoritmo "13.funcao.soma"
// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 01/06/2023

funcao soma(a,b: inteiro): inteiro
inicio
   retorne a+b


fimfuncao

Var
   num1, num2: inteiro


Inicio
   escreval("Informe o 1º valor: ")
   leia(num1)
   escreval("Informe o 2º valor: ")
   leia(num2)
   escreval("A soma e: ",soma(num1,num2))


Fimalgoritmo