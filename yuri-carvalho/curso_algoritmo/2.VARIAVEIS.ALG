Algoritmo "2.variaveis"

// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 25/05/2023
Var
   palavra: caractere
   num: inteiro
   numQuebrado: real
   bool: logico


Inicio
   palavra<- "Ola, mundo! "
   num<- 12345
   numQuebrado<- 12.5
   bool<- falso

   Escreva(palavra, num, " ",numQuebrado, " ", bool)


Fimalgoritmo