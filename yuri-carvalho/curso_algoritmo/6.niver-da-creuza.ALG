Algoritmo "Atividade-niver-da-Creuza"

// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 27/05/2023
Var
   ano_atual, ano_nasc, idade: inteiro


Inicio
   Escreval("Em que ano estamos?")
   leia(ano_atual)
   Escreval("Em qual ano você nasceu?")
   leia(ano_nasc)
   idade<- ano_atual - ano_nasc
   Escreva("Sua idade: ",idade," anos")


Fimalgoritmo