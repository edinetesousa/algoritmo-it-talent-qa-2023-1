Algoritmo "3.1.entrada-e-operadores"
// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 25/05/2023
Var
   num1, num2: inteiro
   soma: inteiro


Inicio
   escreval("Informe o primeiro numero:")
   leia(num1)
   escreval("Informe o segundo numero:")
   leia(num2)
   soma<- num1 + num2
   escreva("A soma e: ", soma)


Fimalgoritmo