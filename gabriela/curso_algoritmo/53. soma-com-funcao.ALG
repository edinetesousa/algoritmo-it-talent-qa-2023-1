algoritmo "Soma função"

funcao Soma(X, Y: inteiro): inteiro
Inicio
    Retorne X + Y
FimFuncao

var
  V1, V2, S: Inteiro

inicio
  escreval("Digite o primeiro valor: ")
  leia(V1)
  escreval("Digite o segundo valor: ")
  leia(V2)
  S <- Soma(V1, V2)
  escreval("A soma entre", V1, "e ", V2, " é igual a", S)
fimalgoritmo