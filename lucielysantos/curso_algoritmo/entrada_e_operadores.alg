algoritmo "Comando de entrada e operadores"

var
    n1, n1, s: inteiro
    m: real
inicio

    escreval("Digite um número:")
    leia(n1)
    escreval("Digite outro número:")
    leia(n2)
    s<-n1+n2
    m<-(n1+n2)/2
    escreval("A soma entre ",n1, " e", n2, " é ",s)
    escreval("A média entre ",n1, " e", n2, " é ",m)

fimalgoritmo