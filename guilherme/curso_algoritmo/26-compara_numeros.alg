algoritmo "compara valores"
var

valor1, valor2: inteiro

inicio

escreval("Valor um: ")
leia(valor1)
escreval("Valor dois: ")
leia(valor2)

se valor1 > valor2 entao
    escreval("Primeiro valor é maior")
senao se valor1 < valor2 entao
    escreval("Segundo valor é maior")
senao se valor1 = valor2 entao
    escreval("Não existe valor maior, os dois são iguais")
fimse
fimse
fimse

fimalgoritmo