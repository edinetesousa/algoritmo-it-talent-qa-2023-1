algoritmo "71-.alg"
var

  array: vetor[0..7] de inteiro
  contador: inteiro

inicio
  
  para contador de 0 ate 7 faca
    array[contador] <- 999
  fimpara
  
  para contador de 0 ate 7 faca
    escreva(array[contador], " ")
  fimpara
  
  para contador de 0 ate 7 faca
    escreva(contador, " ")
  fimpara


fimalgoritmo