algoritmo "Creuza05"

var
    imprestimo, parcela, valor: real

inicio
    escreva ("Qual valor do imprestimo? R$ ")
    leia (imprestimo)
    escreva ("Quantas parcelas? ", parcela)
    leia (parcela)
    valor <- imprestimo/parcela
    escreva ("O valor de cada parcela sera? ", valor)
    

fimalgoritmo