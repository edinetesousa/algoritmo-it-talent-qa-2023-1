algoritmo "Contaratequanto"

var

    A, B, salto: inteiro

inicio
    A<- 0
    B<- 0
    escreva ("Quer contar atÃ© quanto?")
    leia (B)
    escreva ("Qual o valor do salto")
    leia (salto)
        enquanto (A<=B) faca
        escreval (A)
