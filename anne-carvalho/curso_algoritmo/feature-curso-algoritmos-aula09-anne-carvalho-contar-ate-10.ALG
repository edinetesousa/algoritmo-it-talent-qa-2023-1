//Anne Carvalho

Algoritmo "Conte Ate 10"

Var
   contador : Inteiro

Inicio

   contador <- 0
   Enquanto (contador <= 10) faca
      EscrevaL(contador)
      contador <- contador + 1
   FimEnquanto
Fimalgoritmo
