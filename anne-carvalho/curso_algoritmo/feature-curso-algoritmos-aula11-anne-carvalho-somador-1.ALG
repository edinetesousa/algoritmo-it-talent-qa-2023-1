//Estruturas de Repetição 3 - Curso de Algoritmos #11 - Gustavo Guanabara - Somador
//Anne Carvalho

Algoritmo " Somador "

Var
  N: Inteiro
  C: Inteiro
  S: Inteiro
Inicio

  S <- 0
  
  Para C <- 1 ate 5 faca
   EscrevaL("Digite um valor: ")
   Leia(N)

   S <- S + N 
   C <- C + 1 
  FimPara

  EscrevaL("A soma dos valores e  ", S)


Fimalgoritmo

