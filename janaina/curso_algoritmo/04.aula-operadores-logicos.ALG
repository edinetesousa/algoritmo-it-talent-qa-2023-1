algoritmo "operadores-logicos"

var

   A, B, C: Inteiro
   
inicio


      A <- 2
      B <- 3
      C <- 5
      Escreval (verdadeiro e falso)


      A <- 2
      B <- 3
      C <- 5
      Escreval ((A=B) ou (C>A))

      A <- 2
      B <- 3
      C <- 5
      Escreva (nao((A=B) ou (C>A)))
      
fimalgoritmo