Algoritmo "Entrada_e_Saída"

// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 23/05/2023
Var
   num1: inteiro
   num2: inteiro
   soma: inteiro


Inicio
   Escreval("Informe o primeiro numero:")
   leia(num1)
   Escreval("Informe o segundo numero:")
   leia(num2)
   soma<-num1+num2
   Escreva("A soma e: ", soma)
Fimalgoritmo