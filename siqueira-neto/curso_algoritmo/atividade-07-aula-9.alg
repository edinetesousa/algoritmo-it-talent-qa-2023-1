algoritmo "Atividaide 07"

var

    contador, contador1: inteiro
    

inicio

    contador <- 0
    enquanto (contador <= 10) faca
        escreva(contador, " ")
       contador <- contador + 1
    fimenquanto

    escreval("")

    contador1 <- 10
    enquanto (contador1 >= 0) faca
        escreva(contador1, " ")
       contador1 <- contador1 - 1
    fimenquanto

fimalgoritmo