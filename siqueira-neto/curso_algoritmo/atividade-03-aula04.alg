algoritmo "Atividade 03"

var

    a, b, c: Inteiro

teste: caractere

inicio

    a <-2
    b <-3
    c <- 5

    escreval(a > b)
    escreval(b < c)
    escreval(c >= a)
    escreval(a <= b)
    escreval(b = a)
    escreval(c <> a)

    //Operadores Logicos

    escreval((a > b) E (c > b))
    escreval((a > b) OU (c > b))
    escreval(NAO(a > b))


fimalgoritmo